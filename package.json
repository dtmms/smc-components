{
  "name": "rx-components",
  "version": "0.0.1",
  "description": "A collection of RX branded web components to be used in a Bespoke RX React.js web application.",
  "main": "index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "storybook": "NODE_PATH=src start-storybook -p 9001 -c .storybook",
    "build-storybook": "NODE_PATH=src build-storybook -c .storybook -o .storybook-build",
    "build-dev": "react-scripts build",
    "prebuild": "npm run clean && npm run copy-manifest",
    "build": "BABEL_ENV=production babel src/components --out-dir dist --ignore node_modules/ --copy-files",
    "clean": "node_modules/.bin/rimraf build dist",
    "copy-manifest": "node_modules/.bin/copyfiles package.json .npmrc dist/",
    "predist": "npm run build",
    "dist": "cd dist && npm publish",
    "precommit-validate": "hooks/lint_js || (echo 'Please fix any of the above-mentioned linting errors before finalizing this commit.' && exit 1)",
    "eject": "react-scripts eject",
    "lint": "node_modules/.bin/eslint \"src/**/*.js\"",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "pre-commit": {
    "run": [
      "precommit-validate"
    ],
    "silent": true
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stacklearn/rx-components.git"
  },
  "keywords": [
    "bespoke",
    "react",
    "component",
    "rx"
  ],
  "license": "UNLICENCED",
  "author": "Atom <tomboolean@bespoke-rx.com>",
  "contributors": [
    "Atom <tomboolean@bespoke-rx.com>"
  ],
  "dependencies": {
    "apollo-fetch": "0.7.0",
    "articles": "0.2.2",
    "attadux": "0.1.1",
    "attainia-web-components": "0.3.93",
    "babel-polyfill": "6.26.0",
    "color": "2.0.1",
    "create-react-context": "0.2.1",
    "graphiql": "0.11.11",
    "history": "4.7.2",
    "hoist-non-react-statics": "2.3.1",
    "jwt-decode": "2.2.0",
    "pluralize": "7.0.0",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "16.0.0",
    "react-apollo": "1.4.15",
    "react-async-hoc": "0.4.1",
    "react-dom": "16.0.0",
    "react-fastclick": "3.0.2",
    "react-localization": "0.1.2",
    "react-portal": "4.1.2",
    "react-redux": "5.0.6",
    "react-router-dom": "4.1.2",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.0",
    "react-tooltip": "3.3.1",
    "react-widgets": "4.2.0",
    "redux": "3.7.2",
    "redux-auth-wrapper": "2.0.2",
    "redux-form": "7.0.4",
    "redux-logger": "3.0.6",
    "regenerator-runtime": "0.11.0",
    "spected": "0.6.0",
    "styled-components": "2.1.2",
    "subscriptions-transport-ws": "0.8.3",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.15",
    "@storybook/addon-info": "3.2.13",
    "@storybook/addon-knobs": "3.2.15",
    "@storybook/addon-links": "3.2.15",
    "@storybook/addon-notes": "3.2.15",
    "@storybook/addon-storyshots": "3.2.16",
    "@storybook/react": "3.2.13",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-ramda": "1.4.3",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "copyfiles": "1.2.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-import-resolver-node": "0.3.1",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.1",
    "glob-all": "3.1.0",
    "pre-commit": "1.2.2",
    "prettier": "1.7.0",
    "prettier-eslint-cli": "4.3.2",
    "redoc": "2.0.0-alpha.5",
    "rimraf": "2.6.1"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "babylon",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
  }
}
